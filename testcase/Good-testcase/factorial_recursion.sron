{
    name : factorial

    args : (Int_s val)

    if : { 
        
        condition : ${ val <= 1 }

        return : val
    }

    return : ${ val * factorial( ${ val - 1 }  ) }

}
{

   name : Main
    
   println("\n Factorial of given number :- ", factorial(inputInt("\n Enter the number :- ")))
}